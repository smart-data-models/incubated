    },
    "time": {
      "type": "object",
      "description": "Relationship. The time when the return message was sent.",
      "$ref": "https://smart-data-models.github.io/dataModel.AutonomousMobileRobot/schema.json#/definitions/time"
    },
    "receivedTime": {
      "type": "object",
