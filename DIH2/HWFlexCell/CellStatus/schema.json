{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://github.com/smart-data-models/incubated/tree/master/DIH2/HWFlexCell/CellStatus/schema.json",
    "type": "object",
    "title": "The cell status schema",
    "description": "The cell status schema comprises cell status values.",
    "required": [
        "id",
        "error_code",
        "error_message"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "string",
            "description": "OCB object ID.",
            "default": ""
        },
        "error_code": {
            "$id": "#/properties/error_code",
            "type": "string",
            "description": "String containing error code from cell.",
            "default": ""
        },
        "error_message": {
            "$id": "#/properties/error_message",
            "type": "string",
            "description": "String containing error message from cell.",
            "default": ""
        }
    },
    "additionalProperties": true
}