{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "pieceID": {
      "type": "string"
    },
    "dateCreated": {
      "type": "string"
    },
    "manufacturabilityOnFlexEdge": {
      "type": "string",
	  "enum": [
		"CanPickUpOnly",
		"CannotPickUp",
		"CanProcess"
	  ]
    },
    "timeEstimatedOnFlexEdge": {
      "type": "number"
    },
    "weight": {
      "type": "number"
    },
    "sequenceNumber": {
      "type": "number"
    },
    "refpieceLocation": {
      "type": "string"
    },
    "status": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "type"
  ]
}